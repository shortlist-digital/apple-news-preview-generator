  "dependencies": {
    "babel": "^5.8.34",
    "commander": "^2.9.0",
    "es6-promise": "^3.0.2",
    "fs-extra": "^0.26.2",
    "isomorphic-fetch": "^2.2.0",
    "os-homedir": "^1.0.1",
    "os-tmpdir": "^1.0.1",
    "osenv": "^0.1.3",
